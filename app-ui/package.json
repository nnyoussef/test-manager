{
    "name": "app-ui",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite --host 0.0.0.0",
        "build": " run-p \"build-only {@}\" -- && run-p compress",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --max-warnings=0",
        "lint:report": "eslint . -f html -o eslint-report.html --fix && npx open eslint-report.html",
        "format": "prettier --write src/",
        "compress": "gzipper compress --verbose --brotli ./dist ./web/"
    },
    "dependencies": {
        "axios-observable": "latest",
        "marked": "latest",
        "pinia": "latest",
        "rxjs": "latest",
        "vue": "latest",
        "vue-router": "latest"
    },
    "devDependencies": {
        "dompurify": "^3.3.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-naming-convention": "^0.1.3",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.5",
        "@eslint/js": "^9.39.1",
        "@typescript-eslint/parser": "^8.46.3",
        "typescript-eslint": "^8.46.3",
        "vite-plugin-static-copy": "^3.1.4",
        "@rushstack/eslint-patch": "latest",
        "@tsconfig/node20": "latest",
        "@types/jsdom": "^27.0.0",
        "@types/node": "latest",
        "@typescript-eslint/eslint-plugin": "^8.33.0",
        "@vitejs/plugin-vue": "latest",
        "@vue/cli-plugin-typescript": "^5.0.9",
        "@vue/cli-service": "latest",
        "@vue/eslint-config-prettier": "latest",
        "@vue/eslint-config-typescript": "latest",
        "@vue/tsconfig": "latest",
        "cypress": "^14.4.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-unicorn": "^62.0.0",
        "eslint-plugin-vue": "^10.1.0",
        "fs-extra": "latest",
        "gzipper": "latest",
        "jsdom": "^27.0.0",
        "minify-xml": "^4.5.2",
        "npm-run-all2": "latest",
        "prettier": "latest",
        "rollup-plugin-critical": "latest",
        "rollup-plugin-visualizer": "^6.0.1",
        "typescript": "^5.8.3",
        "unplugin-vue-inspector": "^2.3.2",
        "vite": "7.1.11",
        "vite-plugin-inspect": "^11.3.3",
        "vite-plugin-minify": "^2.1.0",
        "vite-plugin-vue-devtools": "latest",
        "vite-plugin-vue-inspector": "^5.3.2",
        "vue-eslint-parser": "^10.2.0",
        "vue-flow-vis": "^1.2.0",
        "vue-tsc": "latest",
        "web-vitals": "^5.0.3"
    }
}
